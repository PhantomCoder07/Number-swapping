#include <stdio.h>
int main()
{
    int a,b;
    int *x = &a;
    int *y = &b;
    printf("Enter two numbers: ");
    scanf("%d %d",&a,&b);
    printf("Numbers before swapping: %d %d\n",a,b);
    int temp = *x;
    *x = *y;
    *y = temp;
    printf("Numbers after swapping: %d %d\n",a,b);
    return 0;
}
