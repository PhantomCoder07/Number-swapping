#include <stdio.h>
void swap (int *x, int *y)
{
    int temp = *x;
    *x = *y;
    *y = temp;
}
int main()
{
    int a,b;
    printf("Enter two numbers: ");
    scanf("%d %d",&a,&b);
    printf("Numbers before swapping: %d %d\n",a,b);
    swap(&a,&b);
    printf("Numbers after swapping: %d %d\n",a,b);
    return 0;
}
